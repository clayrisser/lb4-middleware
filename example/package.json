{
  "name": "example",
  "private": true,
  "version": "0.1.0",
  "description": "authorization adapter for loopback 4",
  "homepage": "https://github.com/codejamninja/lb4-authorization",
  "bugs": {
    "url": "https://github.com/codejamninja/lb4-authorization/issues",
    "email": "jam@codejam.ninja"
  },
  "license": "MIT",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "files": [
    "dist"
  ],
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/codejamninja/lb4-authorization"
  },
  "scripts": {
    "build": "npm run test && lb-tsc --outDir dist && cp -r src/public dist/src",
    "clean": "git clean -fXd -e \\!node_modules -e \\!node_modules/**/*",
    "clean:purge": "git clean -fXd",
    "format": "prettier --write ./**/*.{json,md,scss,yaml,yml,js,jsx,ts,tsx} --ignore-path .gitignore",
    "lint": "npm run format && tsc --allowJs --noEmit && eslint --ext .ts,.tsx ./",
    "lint:fix": "npm run format && tsc --noEmit && eslint --ext .ts,.tsx --fix ./",
    "prepublish": "npm run build",
    "start": "nodemon",
    "test": "npm run lint"
  },
  "dependencies": {
    "@loopback/boot": "^1.4.0",
    "@loopback/context": "^1.19.0",
    "@loopback/core": "^1.8.1",
    "@loopback/openapi-v3": "^1.6.1",
    "@loopback/repository": "^1.6.1",
    "@loopback/rest": "^1.15.0",
    "@loopback/rest-explorer": "^1.2.1",
    "@loopback/service-proxy": "^1.2.1",
    "core-js": "^3.1.3",
    "lb4-middleware": "^0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@loopback/build": "^1.7.1",
    "@types/node": "^12.0.7",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-preset-jam": "^0.4.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-typescript": "^4.0.0",
    "eslint-config-jam": "^0.2.12",
    "nodemon": "^1.19.1",
    "tsc": "^1.20150623.0",
    "typescript": "^3.5.1"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "dist"
  ]
}
